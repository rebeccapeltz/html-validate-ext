{
    "manifest_version": 2,
    "name": "HTML Validator",
    "description": "This extension will validate HTML on a page",
    "version": "1.0",
    "browser_action": {
        "default_icon": {
            "32": "images/valid128.png"
        },
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}